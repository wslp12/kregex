{
  "name": "reactbase",
  "version": "0.2.1",
  "private": true,
  "main": "dist-electron-main/main.js",
  "repository": "https://github.com/wslp12/kregex.git",
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/material": "^5.6.2",
    "electron-json-storage": "^4.5.0",
    "electron-log": "^4.4.6",
    "electron-updater": "^5.0.1",
    "env-cmd": "^10.1.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.3.0",
    "typescript": "^4.6.3",
    "update-electron-app": "^2.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^13.1.1",
    "@types/electron-json-storage": "^4.5.0",
    "@types/jest": "^27.4.1",
    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.1",
    "@types/react-router-dom": "^5.3.3",
    "@types/testing-library__jest-dom": "^5.9.5",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "babel-jest": "^27.5.1",
    "css-loader": "^6.7.1",
    "electron": "^18.1.0",
    "electron-builder": "^23.0.3",
    "eslint": "^8.13.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^27.5.1",
    "react-test-renderer": "^18.0.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.1.1",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^4.8.1"
  },
  "scripts": {
    "build:local:main": "env-cmd -e main,local npx webpack --config webpack.config.js --watch",
    "build:prod:main": "env-cmd -e main,prod npx webpack --config webpack.config.js",
    "start:local:web": "env-cmd -e renderer,local webpack serve",
    "build:prod:web": "env-cmd -e renderer,prod npx webpack --config webpack.config.js",
    "se": "electron .",
    "pack:elec:win:prod": "yarn build:prod:web && yarn build:prod:main && electron-builder --win",
    "test": "jest",
    "test:cover": "jest --coverage"
  },
  "build": {
    "appId": "reactbase",
    "asar": true,
    "win": {
      "icon": "./public/images/icon.icns",
      "target": [
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "directories": {
      "output": "dist-electron",
      "app": "."
    }
  }
}
